function im_f = My_fillhole(im)
%MY_FILLHOLE 此处显示有关此函数的摘要
%   此处显示详细说明
[M,N] = size(im);
im_f = im;
im_b = [im_f(2:M-1,1),zeros(M-2,N-2),im_f(2:M-1,N)];
im_b = [im_f(1,:);im_b;im_f(M,:)];
im_k = im_b;
%% 求出B
k = 1:3;l = 1:3;B(k,l) = 1;

%% 测地膨胀，提取空洞信息
for i = 1:500
    im_b = mybi_imdilate(im_b,B);
    im_b = logical(min(im_b,im));
end

im_f = logical(1-im_b);
end

